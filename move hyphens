import java.util.*;
class Main
{
    public static void main(String arhs[]){
        Scanner sc= new Scanner(System.in);
         String a=sc.nextLine();
         char c[]=a.toCharArray();
         StringBuilder sb = new StringBuilder();
         for(int i=0;i<a.length();i++){
             if(c[i]=='-'){
                 System.out.print(c[i]);
             }
             else{
                 sb.append(c[i]);
             }
         }
        System.out.print(sb);
    }
}
